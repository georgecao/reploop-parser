// Define the PpsUgcChannel of the pps love channel
// Next field id is: 8.
message PpsUgcChannel {
  //option
  option (entity_option).table = PPS_UGC_CHANNEL_TABLE;
  option (entity_option).entity_name = "pps_ugc_channel";
  option (table_option).hbase_table_name = "qipu_pps_ugc_channel_table";
  option (entity_option).entity_id_type = PPS_UGC_CHANNEL;

  required int64 id = 1 [(column_option).is_auto_set = true];

  optional PpsUgcChannelMetadata metadata = 2 [(field_option).column_group = PPS_UGC_CHANNEL_METADATA, (column_option).column_name = "pucm"];

  optional PpsUgcChannelAuditStatus pps_ugc_channel_audit_status = 3 [(field_option).column_group = PPS_UGC_CHANNEL_AUDIT_STATUS, (column_option).column_name = "pucat"];

  optional PpsUgcChannelStats pps_ugc_channel_stats = 4 [(field_option).column_group = PPS_UGC_CHANNEL_STATS, (column_option).column_name = "puct"];

  optional bool deleted = 5  [default = false, (column_option).has_value = false, (field_option).column_group = DELETED, (column_option).column_name = "del"];

  repeated string tag = 6 [(column_option).has_value = false, (field_option).column_group = TAG, (column_option).column_name = "tag_"];

  // Store <name,value> pair specific terms
  repeated KeyValuePair key_value_pair = 7 [(field_option).column_group = KEY_VALUE_PAIR, (column_option).column_name = "kvp_", (column_option).column_suffix = "${name}"];

  optional int64 last_update_time = 8 [(field_option).column_group = LAST_UPDATE_TIME, (column_option).column_name = "lut", (column_option).is_auto_set = true, (track_option).notracker=true];
}

